{
  "name": "@o1labs/snarkyjs-web",
  "description": "Web bindings for SnarkyJS",
  "version": "0.1.28",
  "license": "Apache-2.0",
  "main": "dist/snarky.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "type-check": "tsc --noEmit",
    "web:build-dev": "webpack --config webpack.web.dev.js --stats-error-details",
    "node:build-dev": "webpack --config webpack.node.dev.js --stats-error-details",
    "web:run": "./scripts/build-and-run-server.sh",
    "node:run": "./scripts/build-and-run-node.sh",
    "web:prepublish": "webpack --config webpack.web.prod.js --stats-error-details",
    "node:prepublish": "webpack --config webpack.node.prod.js --stats-error-details",
    "node:test": "node --experimental-wasm-modules --experimental-modules --experimental-wasm-threads ./node_modules/.bin/jest",
    "format": "prettier --write --ignore-unknown **/*",
    "clean": "rm -rf dist"
  },
  "author": "O(1) Labs",
  "devDependencies": {
    "@babel/core": "^7.15.0",
    "@babel/plugin-transform-runtime": "^7.15.0",
    "@babel/preset-env": "^7.15.0",
    "@types/jest": "^27.0.0",
    "babel-loader": "^8.2.2",
    "clean-webpack-plugin": "^4.0.0-alpha.0",
    "jest": "^27.0.6",
    "node-polyfill-webpack-plugin": "^1.1.4",
    "prettier": "^2.3.2",
    "ts-jest": "^27.0.4",
    "ts-loader": "^9.2.5",
    "tslib": "^2.3.0",
    "typescript": "^4.3.5",
    "webpack": "^5.49.0",
    "webpack-cli": "^4.7.2",
    "copy-webpack-plugin": "^9.0.1"
  },
  "dependencies": {
    "env": "^0.0.2",
    "reflect-metadata": "^0.1.13"
  }
}
